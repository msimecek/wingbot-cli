{{#if isAzure}}
{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "appName": {
            "value": "{{azureFunctionAppName}}"
        },
        "botSku": {
            "value": "{{bsBotSku}}"
        },
        "botName": {
            "value": "{{bsBotName}}"
        },
        "botDisplayName": {
            "value": "{{bsBotDisplayName}}"
        },
        "botAppId": {
            "value": "{{bsAppId}}"
        },
        "botAppPassword": {
            "value": "{{bsAppPassword}}"
        },{{#if cosmosdbName}}
        "cosmosDbName": {
            "value": "{{cosmosdbName}}"
        },{{/if}}
        "cosmosDbConnectionString": {
            "value": "{{cosmosdbConnectionString}}"
        }
    }
}
{{/if}}