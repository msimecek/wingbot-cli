'use strict';

{{#if dynamodbStorage}}
const { BotConfigStorage } = require('wingbot-dynamodb');
const config = require('../config');
const dynamodb = require('./dynamodb');

module.exports = new BotConfigStorage(`${config.prefix}-botconfig`, dynamodb);
